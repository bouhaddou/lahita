
<div class="container">
{{ form_start(form) }}

    <div class="form-group">
        {{ form_row(form.libelle) }}
    </div>
    <div class="form-group">
        {{ form_row(form.observation) }}
    </div>
    <div class="form-group">
        {{ form_row(form.date) }}
    </div>
    <button class="btn btn-success"><i class="fas fa-plus"></i> {{ button_label|default('Ajouter') }}</button>
    <a class="btn btn-primary" href="{{ path('categories_index') }}"><i class="fas fa-undo-alt"></i> Annuler</a>
{{ form_end(form) }}
</div>